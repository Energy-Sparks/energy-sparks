<tr class="<%= 'target-final-row' if final_row %>">
  <td class="<%= 'target-final-row' if final_row %>">
    <%= title %>
  </td>
  <% keys.each do |key| %>
    <td class="text-right <%= target_percent_cell_colour(data[key]) if units == :relative_percent %><%= ' partial-month' if partial_months[key] %>">
      <%= format_target(data[key], units) %>
    </td>
  <% end %>
</tr>
