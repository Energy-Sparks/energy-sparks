<%= t('analytics.benchmarking.content.optimum_start_analysis.introduction_text_html') %>
<%= t('analytics.benchmarking.content.optimum_start_analysis.table_introduction_text_html') %>
<%= component 'comparison_table',
              report: report,
              advice_page: nil,
              table_name: table_name,
              index_params: index_params,
              headers: @headers_optimum_start_analysis do |c| %>
  <% @results.each do |result| %>
    <% c.with_row do |row| %>
      <% row.with_school school: result.school %>
      <% row.with_var { result.average_start_time_hh_mm_to_time_of_day.to_s } %>
      <% row.with_var val: result.start_time_standard_devation, unit: Float %>
      <% row.with_var val: result.rating, unit: Float %>
      <% row.with_var val: result.regression_start_time, unit: Float %>
      <% row.with_var val: result.optimum_start_sensitivity, unit: Float %>
      <% row.with_var val: result.regression_r2, unit: Float %>
      <% row.with_var { result.avg_week_start_time_to_time_of_day.to_s } %>
    <% end %>
  <% end %>
<% end %>
<%= t('analytics.benchmarking.content.optimum_start_analysis.caveat_text_html') %>
