<% @table_names.each_with_index do |table_name, index| %>
  <%= render table_name.to_s,
             results: results,
             report: report,
             advice_page: advice_page,
             table_name: table_name,
             index_params: index_params,
             headers: headers,
             colgroups: colgroups,
             advice_page_tab: advice_page_tab,
             table_number: index + 1 %>
<% end %>
