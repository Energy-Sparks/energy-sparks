<div id="<%= id %>" class="prompt-list-component <%= classes %>">
  <% if title? || link? %>
    <div class="row">
      <div class="col d-flex flex-wrap justify-content-between align-items-center">
        <% if title? %>
          <%= title %>
        <% end %>
        <% if link? %>
          <div>
            <%= link %>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
  <% prompts.each do |prompt| %>
    <%= prompt %>
  <% end %>
</div>
